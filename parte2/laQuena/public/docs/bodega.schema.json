{
	"type":"object",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "laquena.herokuapp.com/public/data/establecimiento.json",

	"properties":{
		"establecimientoId": {
			"type":"number"

		},
		"name": {
			"type":"string"
		},
		"productos": {
			"type":"array",
			"items":
				{
					"type":"object",
					"properties":{
						"name": {
							"type":"string"
						},
						"price": {
							"type":"object",
							"properties":{
								"currency": {
									"type":"string"
								},
								"value": {
									"type":"number"
								}
							}
						},
						"productoID": {
							"type":"number"

						},
						"stock": {
							"type":"number"

						}
					}
				}


		}
	}
}
